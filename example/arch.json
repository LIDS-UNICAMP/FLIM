{
    "features": {
        "type": "sequential",
        "layers": {
            "m_norm": {
                "operation": "marker_based_norm",
                "params": {
                    "kernel_size": 5,
                    "dilation": 1
                }
            },
            "conv": {
                "operation": "conv2d",
                "params": {
                    "kernel_size": 5,
                    "stride": 1,
                    "padding": 2,
                    "dilation": 1,
                    "out_channels": 64,
                    "number_of_kernels_per_marker": 64
                }
            },

            "activation": {
                "operation": "relu",
                "params": {
                    "inplace": true
                }
            },

            "pool": {
                "operation": "max_pool2d",
                "params": {
                    "kernel_size": 3,
                    "stride": 1,
                    "padding": 1
                }
            },

            "norm1": {
                "operation": "batch_norm2d",
                "params": {}
            },

            "m_norm2": {
                "operation": "marker_based_norm",
                "params": {
                    "kernel_size": 5,
                    "dilation": 1
                }
            },
            "conv2": {
                "operation": "conv2d",
                "params": {
                    "kernel_size": 5,
                    "stride": 1,
                    "padding": 2,
                    "dilation": 1,
                    "out_channels": 64,
                    "number_of_kernels_per_marker": 64
                }
            },
            "activation2": {
                "operation": "relu",
                "params": {
                    "inplace": true
                }
            }
        }
    }
}